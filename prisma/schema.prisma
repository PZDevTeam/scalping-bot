generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Operations {
  id              Int      @id @default(autoincrement())
  token_mint      String
  token_name      String 
  amount          Float
  usd_price       Float
  total_value     Float
  opetation       String
  pool_id         String
  dex             String
  slot            BigInt
  created_at       DateTime @default(now())
}
